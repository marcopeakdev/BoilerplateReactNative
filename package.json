{
    "scripts": {
        "android": "npx react-native run-android",
        "ios": "npx react-native run-ios",
        "start": "npx react-native start --reset-cache",
        "format": "npm-run-all format:*",
        "format:js": "prettier --write {.,**}/*.js",
        "format:json": "prettier --write {.,**}/*.json",
        "format:md": "prettier --write {.,**}/*.md",
        "format:ts": "prettier --write {.,**}/*.{ts,tsx}",
        "test": "jest",
        "link": "npx react-native link",
        "apk": "cd android && ./gradlew assembleRelease && cd ..",
        "aab": "cd android && ./gradlew bundleRelease && cd ..",
        "lint": "eslint index.js app --fix --ext .js,.ts,.tsx",
        "pod": "cd ios && pod install && cd ..",
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.9.0",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/netinfo": "^5.8.0",
        "@react-navigation/bottom-tabs": "^5.2.8",
        "@react-navigation/drawer": "^5.5.1",
        "@react-navigation/material-top-tabs": "^5.1.10",
        "@react-navigation/native": "^5.1.7",
        "@react-navigation/stack": "^5.2.14",
        "axios": "^0.19.2",
        "i18next": "^19.4.4",
        "moment": "^2.24.0",
        "react-fast-compare": "^3.1.1",
        "immutable": "^4.0.0-rc.12",
        "react-i18next": "^11.4.0",
        "react-native-modal": "^11.5.6",
        "react-native-dropdownalert": "^4.2.1",
        "react-native-fast-image": "^8.1.5",
        "react-native-gesture-handler": "^1.6.1",
        "react-native-keychain": "^6.0.0",
        "react-native-reanimated": "^1.8.0",
        "react-native-redash": "^12.6.1",
        "react-native-safe-area-context": "^0.7.3",
        "react-native-safe-area-view": "^1.1.1",
        "react-native-screens": "^2.4.0",
        "react-native-splash-screen": "^3.2.0",
        "react-native-tab-view": "^2.14.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-form": "^8.3.5",
        "redux-immutable": "^4.0.0",
        "redux-logger": "^3.0.6",
        "redux-saga": "^1.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "@react-native-community/eslint-config": "^1.1.0",
        "@types/jest": "^25.2.1",
        "@types/react-native": "^0.62.4",
        "@types/redux-immutable":"4.0.1",
        "@types/react-redux": "^7.1.7",
        "@types/react-test-renderer": "^16.9.2",
        "@types/redux-form": "^8.2.3",
        "@types/redux-logger": "^3.0.7",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "babel-jest": "^25.4.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "eslint": "^6.8.0",
        "jest": "^25.4.0",
        "metro-react-native-babel-preset": "^0.59.0",
        "react-test-renderer": "^16.13.1",
        "typescript": "^3.8.3"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
