{
  "name": "HelloWorld",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "splash": "yarn react-native generate-bootsplash splash/splash.png --background-color=FFFFFF --logo-width=150  --assets-path=assets --flavor=main",
    "clean-ios": "cd ios && xcodebuild clean && cd ..",
    "android:dev": "node ./scripts/android.js env/.dev devDebug",
    "android:dev:release": "node ./scripts/android.js env/.dev devRelease",
    "android:prod": "node ./scripts/android.js env/.prod prodDebug",
    "ios:dev": "node ./scripts/ios.js env/.dev",
    "ios:prod": "node ./scripts/ios.js env/.prod",
    "start": "npx react-native start --reset-cache",
    "format": "prettier --write **/*.{ts,tsx,js,json,md}",
    "type:check": "yarn tsc --noEmit --skipLibCheck",
    "test": "jest",
    "build:staging": "fastlane ios staging_build --env dev && fastlane android staging_build --env dev",
    "lint": "eslint  index.js ./__tests__/** ./scripts/*.js  ./src/** --ext .js,.ts,.tsx",
    "postinstall": "node ./scripts/setup.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint  --ext"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^8.2.0",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.7.2",
    "axios": "^0.26.0",
    "i18next": "^20.4.0",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-hook-form": "^7.28.1",
    "react-i18next": "^11.11.4",
    "react-native": "0.67.4",
    "react-native-blurhash": "^1.1.10",
    "react-native-bootsplash": "^4.1.3",
    "react-native-config": "^1.4.5",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.66.4",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.2.2",
    "metro-react-native-babel-preset": "^0.66.2",
    "patch-package": "^6.4.7",
    "prettier": "^2.3.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.2.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
