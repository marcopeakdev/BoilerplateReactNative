{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && xcodebuild clean && cd ..",
    "android": "yarn clean-android && npx react-native run-android ",
    "ios": "yarn clean-ios && npx react-native run-ios",
    "yarn-ios": "yarn && yarn pod",
    "android-release": "clean-android && npx react-native run-android --variant=release",
    "start": "npx react-native start --reset-cache",
    "format": "prettier --write **/*.{ts,tsx,js,json,md}",
    "check-code": "prettier --check **/*.{ts,tsx,js,json,md}",
    "test": "jest",
    "link": "npx react-native link",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "lint": "eslint App.tsx index.js ./src/app/* --ext .js,.ts,.tsx",
    "pod": "cd ios && pod install && cd ..",
    "installLib": "yarn add ",
    "installPod": "func() { npm run installLib \"$1\" && npm run pod; }; func",
    "yp": "npm run installPod -- "
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.7",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.1",
    "i18next": "^20.4.0",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-hook-form": "^7.12.2",
    "react-i18next": "^11.11.4",
    "react-native": "0.64.2",
    "react-native-blurhash": "^1.1.5",
    "react-native-bootsplash": "^3.2.5",
    "react-native-fast-image": "^8.3.7",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keyboard-manager": "^6.5.4-2",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.5.0",
    "react-native-sensitive-info": "^6.0.0-alpha.9",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-svg": "^12.1.1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.63.51",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.3.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.2.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
